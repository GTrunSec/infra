{
  "job": {
    "vast": {
      "datacenters": ["dc1"],
      "group": {
        "nixos": {
          "count": 1,
          "task": {
            "vast": {
              "config": {
                "nixos": "/nix/store/g54lcqva0vy54rs06nnxiasmjlkckykl-source#x86_64-linux.tenzir.nixosProfiles.nomad-tenzir-vast"
              },
              "driver": "nix",
              "resources": { "cpu": 3000, "memory": 1100 },
              "volume_mount": {
                "destination": "/var/lib/private/vast",
                "read_only": false,
                "volume": "vast"
              }
            }
          },
          "volume": {
            "vast": { "read_only": false, "source": "vast", "type": "host" }
          }
        }
      },
      "namespace": "default",
      "type": "batch"
    }
  }
}
